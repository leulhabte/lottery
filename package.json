{
  "name": "express-rest",
  "version": "0.0.1",
  "description": "Boilerplate for node, express, mongoose REST API",
  "main": "server.js",
  "author": "abdulhamidOumer",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node -e js",
    "compile": "babel-node src/ethereum/compile.js",
    "deploy": "babel-node src/ethereum/deploy.js",
    "start:prod": "yarn build && NODE_ENV=production node build/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "yarn clean && yarn build-babel",
    "lint": "eslint . --ext .js --color -f table",
    "test": "mocha test --require @babel/register --recursive",
    "coverage": "nyc yarn test",
    "seed": "babel-node src/database/seeders/index.js",
    "task": "babel-node src/tasks/daily/index.js"
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@adminjs/express": "^5.0.1",
    "@adminjs/mongoose": "^3.0.0",
    "@babel/preset-react": "^7.18.6",
    "@truffle/hdwallet-provider": "^2.0.16",
    "adminjs": "^6.2.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.6.1",
    "fs-extra": "^10.1.0",
    "ganache-cli": "^6.12.2",
    "helmet": "^4.1.1",
    "http-status": "^1.4.2",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "minimist": "^1.2.6",
    "mongoose": "^5.10.8",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.4",
    "solc": "^0.8.17",
    "tslib": "^2.4.0",
    "web3": "^1.8.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-watch": "^7.0.0",
    "husky": "4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2"
  }
}
